{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "scripts": {
    "dev": "npm run build-vendor:dev && npm run build:dev",
    "build:dev": "webpack --config webpack.config.js --progress",
    "build-vendor:dev": "webpack --config webpack.config.vendor.js --progress",
    "build": "npm run build-vendor:prod && npm run build:prod",
    "build:prod": "webpack --config webpack.config.js --progress --hide-modules --env.prod",
    "build-vendor:prod": "webpack --config webpack.config.vendor.js --progress --env.prod",
    "lint": "eslint -c ./.eslintrc.js ClientApp/**/*.js  ClientApp/**/*.vue  ClientApp/**/*.json webpack*.js"
  },
